      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false
    },
    {
      "minLeght": 2,
      "name": "day",
      "type": "NUMBER",
      "displayWhenZero": true,
      "formatting": [],
      "hideSign": false
    }
  ]
